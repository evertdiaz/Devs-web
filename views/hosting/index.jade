                <button class="c-search-toggler" type="button">
                <i class="fa fa-search"></i>
                </button>
            // END: BRAND
            // BEGIN: QUICK SEARCH
            form.c-quick-search(action='#')
              input.form-control(type='text', name='query', placeholder='Type to search...', value='', autocomplete='off')
