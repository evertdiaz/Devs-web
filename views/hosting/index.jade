block content
  body.c-layout-header-fixed
    // BEGIN: LAYOUT/HEADERS/HEADER-1
    // BEGIN: HEADER
    header.c-layout-header.c-layout-header-3.c-layout-header-3-custom-menu.c-layout-header-default-mobile
      .c-navbar
        .container
          // BEGIN: BRAND
          .c-navbar-wrapper.clearfix
            .c-brand.c-pull-left
              a.c-logo(href='/')
                img.c-desktop-logo(src='../../assets/base/img/logos/logo-1.png', alt='Logotipo de Devs Imagen Digital')
                img.c-desktop-logo-inverse(src='../../assets/base/img/logos/logo-1.png', alt='Logotipo de Devs Imagen Digital')
                img.c-mobile-logo(src='../../assets/base/img/logos/logo-1.png', alt='Logotipo de Devs Imagen Digital')
