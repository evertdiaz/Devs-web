extends layout

block content
  body.c-layout-header-fixed.c-layout-header-fullscreen.c-layout-header-topbar
    // BEGIN: LAYOUT/HEADERS/HEADER-1
    // BEGIN: HEADER
    header.c-layout-header.c-layout-header-default.c-layout-header-default-mobile
      .c-navbar
        .container
          // BEGIN: BRAND
          .c-navbar-wrapper.clearfix
            .c-brand.c-pull-left
              a.c-logo(href='/')
                img.c-desktop-logo(src='assets/base/img/logos/logo-1.png', alt='Logo de Devs')
                img.c-desktop-logo-inverse(src='assets/base/img/logos/logo-1.png', alt='Logo de Devs')
                img.c-mobile-logo(src='assets/base/img/logos/logo-1.png', alt='Logo de Devs')
              button.c-hor-nav-toggler(type='button', data-target='.c-mega-menu')
                span.c-line
                span.c-line
                span.c-line
              //
                PROXIMO LANZMAIENTO BUSQUEDA
                <button class="c-search-toggler" type="button">
                <i class="fa fa-search"></i>
                </button>
            // END: BRAND
            // BEGIN: QUICK SEARCH
            form.c-quick-search(action='#')
              input.form-control(type='text', name='query', placeholder='Type to search...', value='', autocomplete='off')
              span.c-theme-link ×
            // END: QUICK SEARCH
            // BEGIN: HOR NAV
            // BEGIN: LAYOUT/HEADERS/MEGA-MENU
            // BEGIN: MEGA MENU
            nav.c-mega-menu.c-pull-right.c-mega-menu-dark.c-mega-menu-dark-mobile.c-fonts-uppercase.c-fonts-bold
              // BEGIN: MEGA MENU
              ul.nav.navbar-nav.c-theme-nav
                li.c-active.c-menu-type-classic
                  a.c-link(href='/') Inicio
                li.c-menu-type-classic
                  a.c-link.dropdown-toggle Servicios
                  ul.dropdown-menu.c-menu-type-classic.c-pull-left
                    li
                      a(href='/web') Desarrollo Web
                    li
                      a(href='/hosting') Hosting
                    //
                      PROXIMO LANZAMIENTO
                      <li class="dropdown-submenu">
                      <a href="servicios/posicionamiento/index.html">Posicionamiento</a>
                      </li>
                      <li class="dropdown-submenu">
                      <a href="servicios/redes-sociales/index.html">Redes Sociales</a>
                      </li>
                      <li class="dropdown-submenu">
                      <a href="servicios/consultoria/index.html">Consultoría</a>
                      </li>
                //
                  PROXIMO LANZAMIENTO
                  <li class="c-menu-type-classic">
                  <a href="#" class="c-link">Blog</a>
                  </li>
                  <li class="c-menu-type-classic">
                  <a href="#" class="c-link">Arma tu producto</a>
                  </li>
                li.c-menu-type-classic
                  a.c-link(href='/contacto') Contacto
                //
                  OTROS ELEMENTOS
                  <li class="c-search-toggler-wrapper">
                  <a href="#" class="c-btn-icon c-search-toggler"><i class="fa fa-search"></i></a>
                  </li>
                  <li>
                  <a href="javascript:;" data-toggle="modal" data-target="#login-form" class="c-btn-border-opacity-04 c-btn btn-no-focus c-btn-header btn btn-sm c-btn-border-1x c-btn-white c-btn-circle c-btn-uppercase c-btn-sbold"><i class="icon-user"></i> Sign In</a>
                  </li>
                  <li class="c-quick-sidebar-toggler-wrapper">
                  <a href="#" class="c-quick-sidebar-toggler">
                  <span class="c-line"></span>
                  <span class="c-line"></span>
                  <span class="c-line"></span>
                  </a>
                  </li>
              // END MEGA MENU
            // END: MEGA MENU
            // END: LAYOUT/HEADERS/MEGA-MENU
            // END: HOR NAV
    // END: HEADER
    // END: LAYOUT/HEADERS/HEADER-1
    // BEGIN: CONTENT/USER/FORGET-PASSWORD-FORM
    #forget-password-form.modal.fade.c-content-login-form(role='dialog')
      .modal-dialog
        .modal-content.c-square
          .modal-header.c-no-border
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') ×
          .modal-body
            h3.c-font-24.c-font-sbold Password Recovery
            p
              | To recover your password please fill in your email address
            form
              .form-group
                label.hide(for='forget-email') Email
                input#forget-email.form-control.input-lg.c-square(type='email', placeholder='Email')
              .form-group
                button.btn.c-theme-btn.btn-md.c-btn-uppercase.c-btn-bold.c-btn-square.c-btn-login(type='submit') Submit
                a.c-btn-forgot(href='javascript:;', data-toggle='modal', data-target='#login-form', data-dismiss='modal') Back To Login
          .modal-footer.c-no-border
            span.c-text-account Don't Have An Account Yet ?
            a.btn.c-btn-dark-1.btn.c-btn-uppercase.c-btn-bold.c-btn-slim.c-btn-border-2x.c-btn-square.c-btn-signup(href='javascript:;', data-toggle='modal', data-target='#signup-form', data-dismiss='modal') Signup!
    // END: CONTENT/USER/FORGET-PASSWORD-FORM
    // BEGIN: CONTENT/USER/SIGNUP-FORM
    #signup-form.modal.fade.c-content-login-form(role='dialog')
      .modal-dialog
        .modal-content.c-square
          .modal-header.c-no-border
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') ×
          .modal-body
            h3.c-font-24.c-font-sbold Create An Account
            p
              | Please fill in below form to create an account with us
            form
              .form-group
                label.hide(for='signup-email') Email
                input#signup-email.form-control.input-lg.c-square(type='email', placeholder='Email')
              .form-group
                label.hide(for='signup-username') Username
                input#signup-username.form-control.input-lg.c-square(type='email', placeholder='Username')
              .form-group
                label.hide(for='signup-fullname') Fullname
                input#signup-fullname.form-control.input-lg.c-square(type='email', placeholder='Fullname')
              .form-group
                label.hide(for='signup-country') Country
                select#signup-country.form-control.input-lg.c-square
                  option(value='1') Country
              .form-group
                button.btn.c-theme-btn.btn-md.c-btn-uppercase.c-btn-bold.c-btn-square.c-btn-login(type='submit') Signup
                a.c-btn-forgot(href='javascript:;', data-toggle='modal', data-target='#login-form', data-dismiss='modal') Back To Login
    // END: CONTENT/USER/SIGNUP-FORM
    // BEGIN: CONTENT/USER/LOGIN-FORM
    #login-form.modal.fade.c-content-login-form(role='dialog')
      .modal-dialog
        .modal-content.c-square
          .modal-header.c-no-border
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') ×
          .modal-body
            h3.c-font-24.c-font-sbold Good Afternoon!
            p
              | Let's make today a great day!
            form
              .form-group
                label.hide(for='login-email') Email
                input#login-email.form-control.input-lg.c-square(type='email', placeholder='Email')
              .form-group
                label.hide(for='login-password') Password
                input#login-password.form-control.input-lg.c-square(type='password', placeholder='Password')
              .form-group
                .c-checkbox
                  input#login-rememberme.c-check(type='checkbox')
                  label.c-font-thin.c-font-17(for='login-rememberme')
                    span
                    span.check
                    span.box
                    |               Remember Me
              .form-group
                button.btn.c-theme-btn.btn-md.c-btn-uppercase.c-btn-bold.c-btn-square.c-btn-login(type='submit') Login
                a.c-btn-forgot(href='javascript:;', data-toggle='modal', data-target='#forget-password-form', data-dismiss='modal') Forgot Your Password ?
              .clearfix
                .c-content-divider.c-divider-sm.c-icon-bg.c-bg-grey.c-margin-b-20
                  span(style='width: 110px') or signup with
                ul.c-content-list-adjusted
                  li
                    a.btn.btn-block.c-btn-square.btn-social.btn-twitter
                      i.fa.fa-twitter
                      |                 Twitter
                  li
                    a.btn.btn-block.c-btn-square.btn-social.btn-facebook
                      i.fa.fa-facebook
                      |                 Facebook
                  li
                    a.btn.btn-block.c-btn-square.btn-social.btn-google
                      i.fa.fa-google
                      |                 Google
          .modal-footer.c-no-border
            span.c-text-account Don't Have An Account Yet ?
            a.btn.c-btn-dark-1.btn.c-btn-uppercase.c-btn-bold.c-btn-slim.c-btn-border-2x.c-btn-square.c-btn-signup(href='javascript:;', data-toggle='modal', data-target='#signup-form', data-dismiss='modal') Signup!
    // END: CONTENT/USER/LOGIN-FORM
    // BEGIN: LAYOUT/SIDEBARS/QUICK-SIDEBAR
